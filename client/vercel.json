{

  "redirects": [
    {

      "source": "/(.*)",
      "has": [{ "type": "host", "value": "fantasynamecreator.com" }],
      "destination": "https://www.fantasynamecreator.com/$1",
      "permanent": true
    },
    {

      "source": "/(.*)",
      "has": [{ "type": "header", "key": "x-forwarded-proto", "value": "http" }],
      "destination": "https://www.fantasynamecreator.com/$1",
      "permanent": true
    }
  ],
  
  "rewrites": [
    {
      "source": "/(.*)", 
      "destination": "/" 
    }
  ]
}
