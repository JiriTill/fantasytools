{
    "version": 2,
    "buildCommand": "npm run build",
    "installCommand": "npm run install:all",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/server/index.js"
        },
        {
            "source": "/(.*)",
            "destination": "/client/dist/$1"
        }
    ]
}